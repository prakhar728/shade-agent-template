    "@near-js/signers": "^2.4.0",
    "@near-js/tokens": "^2.4.0",
    "@neardefi/shade-agent-js": "^1.0.0",
    "@pythnetwork/pyth-evm-js": "^1.29.2",
    "@uniswap/sdk-core": "^3.1.0",
    "@uniswap/v3-sdk": "^3.9.0",
    "big.js": "^6.1.1",
    "binance-api-node": "^0.11.13",
    "chainsig.js": "^1.1.11",
    "coingecko-api-v3": "0.0.13",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ethers": "^6.11.1",
    "hono": "^4.8.4",
    "messaging-api-telegram": "^1.1.0",
    "near-api-js": "^0.42.0",
